{
    "name": "API.AI Telegram integration",
    "description": "Api.ai Telegram integration allows you to create Telegram bots with natural language understanding based on Api.ai technology.",
    "repository": "https://github.com/indraakkk/simple-telegram-chatbot",
    "logo": "http://xvir.github.io/img/apiai.png",
    "keywords": ["api.ai", "telegram", "natural language"],
    "env": {
      "APP_NAME": {
        "description": "indra-col-bot",
        "value": ""
      },
      "TELEGRAM_TOKEN": {
        "description": "Telegram bot token",
        "value": ""
      },
      "APIAI_ACCESS_TOKEN": {
        "description": "Client access token for Api.ai",
        "value": ""
      },
      "APIAI_LANG": {
        "description": "Agent language",
        "value": "en",
        "required": false
      }
    },
    "engines": {
      "node": "8.11.3"
    }
  }